                        <i class="fas fa-user-graduate aluno-big"></i>
                    </div>
                    <h5 class="title-topics">Treinamento</h5>
                </a>
            </div>

            <div class="col-12 col-md-2 col-lg-2 d-flex justify-content-center ball-mobile">
                <a data-toggle="modal" data-target="#operacao-modal">
                    <div class="ball-off ball-04" data-aos="fade-right" data-aos-duration="500" data-aos-delay="300">
                        <i class="far fa-eye olho-big"></i>
                    </div>
                    <h5 class="title-topics">Operação Assistida</h5>
                </a>
            </div>

            <div class="col-12 col-md-2 col-lg-2 d-flex justify-content-center ball-mobile">
                <a data-toggle="modal" data-target="#governanca-modal">
                    <div class="ball-off ball-05" data-aos="fade-right" data-aos-duration="500" data-aos-delay="400" style="margin-left: 25px;">
                        <i class="fas fa-check visto-big"></i>
                    </div>
                    <h5 class="title-topics">Governança da Operação</h5>
                </a>
            </div>


        </div>
    </div>

    <div class="container-fluid cadastro">
        <div class="row">
            <div class="col-12 d-flex justify-content-center">
                <h1 class="line-height">
                    Cadastro de dispositivos<br />
                    ágil e descomplicado
                </h1>
            </div>
            <div class="col-12 d-flex justify-content-center">
                <div class="image-containers">
